{
  "name": "davegthemighty/shopping",
  "description": "Sample App",
  "authors": [
    {
      "name": "Dave Gillard",
      "email": "davegthemighty@hotmail.com"
    }
  ],
  "config": {
      "sort-packages": true
  },
  "require": {
    "php": ">=7.3",
    "illuminate/database": "6.*",
    "monolog/monolog": "^1.24",
    "ramsey/uuid": "^3.8",
    "robmorgan/phinx": "^0.11.1",
    "slim/slim": "^3.0",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "DavegTheMighty\\ShoppingBasket\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "DavegTheMighty\\ShoppingBasket\\Test\\": "tests/"
      }
  },
  "minimum-stability": "stable"
}
